//leetcode submit region begin(Prohibit modification and deletion)
class Solution {
    public boolean isOneBitCharacter(int[] bits) {
        for (int i=0;i<bits.length;i++) {
            if (bits[i] == 1) {
                i++;
                if (i == bits.length-1) {
                    return false;
                }
            }
        }
        return true;
    }
}
//leetcode submit region end(Prohibit modification and deletion)